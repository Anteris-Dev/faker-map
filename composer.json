{
    "name": "anteris-dev/faker-map",
    "description": "Guesses which Faker provider method you might need based on the name passed.",
    "require": {
        "php": "^8.0",
        "fakerphp/faker": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "vimeo/psalm": "^4.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Aidan Casey",
            "email": "aidan.casey@anteris.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Anteris\\FakerMap\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anteris\\Tests\\FakerMap\\": "./tests"
        }
    },
    "scripts": {
        "fix": "./vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": "./vendor/bin/phpunit"
    }
}
